
var baseNode = "SpriteNode";

global vel;
global velMax = 7.5f;
global velMin = -10.0f;

def Initialize() {
    vel = 0.0f;
}

def Update(delta) {
    vel -= 0.5;
    if(KeyPressed(" ")) {
        vel += 1;
    }

    vel = max(vel, velMin);
    vel = min(vel, velMax);

    position.y += vel * delta;
    rotation = max(vel / velMax, -3.14 / 2);
}
